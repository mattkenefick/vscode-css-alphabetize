{
	"activationEvents": [
		"onCommand:css-alphabetize.alphabetize"
	],
	"author": {
		"email": "matt@polymermallard.com",
		"name": "Matt Kenefick"
	},
	"bugs": {
		"url": "https://github.com/mattkenefick/vscode-css-alphabetize/issues"
	},
	"categories": [
		"CSS",
		"Other"
	],
	"contributes": {
		"commands": [
			{
				"command": "css-alphabetize.alphabetize",
				"title": "CSS/SASS: Alphabetize properties"
			}
		],
		"keybindings": [
			{
				"command": "extension.css-alphabetize",
				"key": "shift+alt+a",
				"mac": "shift+alt+a"
			}
		]
	},
	"description": "Alphabetizes the properties of CSS structures.",
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"displayName": "CSS Alphabetize",
	"engines": {
		"vscode": "^1.57.0"
	},
	"galleryBanner": {
		"color": "#d9d9d9",
		"theme": "light"
	},
	"homepage": "https://github.com/mattkenefick/vscode-css-alphabetize",
	"icon": "assets/logo.png",
	"license": "MIT",
	"main": "./dist/extension.js",
	"name": "css-alphabetize",
	"publisher": "PolymerMallard",
	"repository": {
		"type": "git",
		"url": "https://github.com/mattkenefick/vscode-css-alphabetize.git"
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"version": "0.1.0"
}
